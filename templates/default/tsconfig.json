{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": false,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": ["DOM", "ESNext"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "paths": {
      "@/api": ["src/api/index.ts"],
      "@/atoms": ["src/components/atoms/index.ts"],
      "@/constants": ["src/constants/index.ts"],
      "@/hooks": ["src/hooks/index.ts"],
      "@/icons": ["src/components/atoms/icons/index.ts"],
      "@/molecules": ["src/components/molecules/index.ts"],
      "@/organisms": ["src/components/organisms/index.ts"],
      "@/styles": ["src/styles/index.ts"],
      "@/styles/global.css": ["src/styles/global.css"],
      "@/templates": ["src/components/templates/index.ts"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES5"
  },
  "exclude": [],
  "include": ["next-env.d.ts", "src/**/*"]
}
